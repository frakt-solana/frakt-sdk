{
  "name": "@frakt-protocol/frakt-sdk",
  "version": "0.2.2",
  "description": "Frakt SDK for interacting with frakt.xyz protocols",
  "keywords": [
    "Frakt",
    "Solana"
  ],
  "author": "Vedamir Efanov",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc src/index.ts",
    "prepub": "npm run build && npm run docs",
    "minor": "npm version minor && npm run prepub && npm publish",
    "patch": "npm version patch && npm run prepub && npm publish"
  },
  "dependencies": {
    "@project-serum/anchor": "0.24.2",
    "@raydium-io/raydium-sdk": "1.1.0-beta.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.3.1",
    "@types/jest": "^27.4.1",
    "@types/mz": "^2.7.4",
    "@types/node": "^17.0.23",
    "@types/text-encoding": "^0.0.36",
    "chai": "^4.3.6",
    "jest": "^27.5.1",
    "mz": "^2.7.0",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.16",
    "typescript": "^4.6.3"
  }
}
